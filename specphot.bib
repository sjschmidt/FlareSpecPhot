	Keywords = {brown dwarfs, solar neighborhood, stars: kinematics and dynamics, stars: low-mass},
	Month = may,
	Pages = {1808-1821},
	Primaryclass = {astro-ph.SR},
	Title = {{Colors and Kinematics of L Dwarfs from the Sloan Digital Sky Survey}},
