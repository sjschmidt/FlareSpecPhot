	Eprint = {1406.1228},
	Journal = {\pasp},
	Keywords = {Stars},
	Month = jul,
	Pages = {642-659},
	Primaryclass = {astro-ph.SR},
	Title = {{Calibrating Ultracool Dwarfs: Optical Template Spectra, Bolometric Corrections, and {$\chi$} Values}},
