	Archiveprefix = {arXiv},
	Author = {{Doi}, M. and {Tanaka}, M. and {Fukugita}, M. and {Gunn}, J.~E. and {Yasuda}, N. and {Ivezi{\'c}}, {\v Z}. and {Brinkmann}, J. and {de Haars}, E. and {Kleinman}, S.~J. and {Krzesinski}, J. and {French Leger}, R.},
	Date-Added = {2017-12-04 13:10:51 +0000},
