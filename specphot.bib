	Doi = {10.1088/0004-6256/149/5/158},
	Eid = {158},
	Eprint = {1410.0014},
	Journal = {\aj},
	Keywords = {astronomical databases: miscellaneous, brown dwarfs, stars: chromospheres, stars: late-type, stars: low-mass},
	Month = may,
